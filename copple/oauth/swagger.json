{
  "swagger": "2.0",
  "info": {
    "title": "My API",
    "version": "1.0.0"
  },
  "basePath": "/",
  "paths": {
    "/login": {
      "get": {
        "summary": "로그인 화면",
        "responses": {
          "200": {
            "description": "로그인 화면"
          }
        }
      }
    },
    "/": {
      "get": {
        "summary": "메인 화면",
        "responses": {
          "200": {
            "description": "메인 화면"
          },
          "302": {
            "description": "로그인하지 않은 사용자인 경우 로그인 화면으로 리다이렉트"
          }
        }
      }
    },
    "/auth/google": {
      "get": {
        "summary": "Google 로그인",
        "responses": {
          "302": {
            "description": "Google 로그인 화면으로 리다이렉트"
          }
        }
      }
    },
    "/auth/google/callback": {
      "get": {
        "summary": "Google 로그인 콜백",
        "responses": {
          "200": {
            "description": "로그인 성공 후, 토큰반환"
          }
        }
      }
    },
    "/profile": {
      "get": {
        "summary": "JWT를 이용하여 사용자 정보 조회",
        "responses": {
          "200": {
            "description": "사용자 정보 조회"
          },
          "401": {
            "description": "인증이 실패한 경우"
          }
        }
      }
    }
  }
}
